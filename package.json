{
	"name": "hexowriter",
	"displayName": "HexoWriter",
	"description": "Plug in for writing hexo",
	"version": "0.0.3",
	"publisher": "xyCode",
	"icon": "image/logo.png",
	"engines": {
		"vscode": "^1.28.0"
	},
	"keywords": [
		"vscode",
		"hexo",
		"writer"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:hexoproj.json"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.hexowriter.commit",
				"title": "HexoWriter Commit"
			},
			{
				"command": "extension.hexowriter.generate",
				"title": "HexoWriter Generate"
			},
			{
				"command": "extension.hexowriter.new",
				"title": "HexoWriter New"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/ssh2": "^0.5.40",
		"@types/vscode": "^1.28.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"ssh2": "^1.4.0"
	},
	"bugs": {
		"url": "https://github.com/xiongyu/hexowriter/issues"
	},
	"repository": {
		"type": "git",
		"url": "hhttps://github.com/xiongyu/hexowriter"
	},
	"homepage": "http://feichanghaowan.com"
}
